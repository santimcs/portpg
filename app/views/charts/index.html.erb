<p id="notice"><%= notice %></p>
<% content_for :title, 'Charts' %>
<h1>Charts</h1>

<table>
  <thead>
    <tr>
      <th>Ticker</th>
      <th>Year</th>
      <th>Quarter</th>
      <th>Image q</th>
      <th>Image y</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @charts.each do |chart| %>
      <tr>
        <td><%= chart.ticker.name %></td>
        <td><%= chart.year %></td>
        <td><%= chart.quarter %></td>
        <td><%= chart.image_q %></td>
        <td><%= chart.image_y %></td>
        <td><%= link_to 'Show', chart %></td>
        <td><%= link_to 'Edit', edit_chart_path(chart) %></td>
        <td><%= link_to 'Destroy', chart, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Chart', new_chart_path %>
